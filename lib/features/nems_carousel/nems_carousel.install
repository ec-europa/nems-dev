<?php
/**
 * @file
 * Hooks executed during module installation.
 */

/**
 * Implements hook_enable().
 */
function nems_carousel_enable() {
  drupal_set_message(t('NEMS carousel feature is now active on your site.'));
}

/**
 * Implements hook_disable().
 */
function nems_carousel_disable() {
  drupal_set_message(t('NEMS carousel feature is now disabled on your site.'));
}
